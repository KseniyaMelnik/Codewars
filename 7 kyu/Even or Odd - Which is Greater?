function evenOrOdd(str) {
let arr = str.split('');
let sumEven = 0; 
let sumOdd = 0;
  for (let i = 0; i< arr.length; i++) {
  if ((Number(arr[i]))%2 == 0) {
   sumEven += Number(arr[i]);
  } else {
   sumOdd += Number(arr[i]);
  }
  }
  if (sumEven > sumOdd) {
    return "Even is greater than Odd";
  }
  if (sumEven < sumOdd) {
    return "Odd is greater than Even";
  }
  if (sumEven == sumOdd) {
    return "Even and Odd are the same";
